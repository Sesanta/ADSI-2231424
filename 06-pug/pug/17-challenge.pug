docytpe
html(lang="es")
	head
		meta(charset="UTF-8")
		title Challenge PUG
		style.
			h1 {
				color: #ddd;
				text-shadow: 0px 2px 4px #333;
			}
			div {
				border: 1px dashed #000;
				height: 50px;
				width: 500px;				
			}
	body
		h1 Challenge PUG
		ul
			each val in ['#ffeaea', '#ffc7c7', '#ffa1a1', '#ff8e8e', '#ff7474', '#ff5959', '#ff4646', '#ff3434', '#ff2222', '#ff0000']
				div(style='background-color:'+val)